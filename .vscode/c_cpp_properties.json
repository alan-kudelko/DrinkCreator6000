{
  "configurations": [
    {
      "name": "AVR",
      "compilerPath": "${workspaceFolder}/tools/bin/avr-g++.exe",
      "intelliSenseMode": "gcc-x64",
      "defines": [
        "F_CPU=16000000UL",
        "__AVR_ATmega2560__"
      ],
      "includePath": [
        "${workspaceFolder}/include",
        "${workspaceFolder}/tools/avr/include",
        "${workspaceFolder}/tools/avr/include/avr",
        "${workspaceFolder}/tools/lib/gcc/avr/7.3.0/include",
        "${workspaceFolder}/FreeRTOS/FreeRTOS-Kernel",        
        "${workspaceFolder}/FreeRTOS/FreeRTOS-Kernel/include",
        "${workspaceFolder}/FreeRTOS/FreeRTOS-Kernel/portable/ThirdParty/GCC/ATmega",
        "${workspaceFolder}/FreeRTOS/FreeRTOS-Kernel/portable/MemMang",

        "${workspaceFolder}/ArduinoCore-avr/libraries/Wire/src",
        "${workspaceFolder}/ArduinoCore-avr/libraries/Wire/src/utility",
        "${workspaceFolder}/ArduinoCore-avr/libraries/Arduino-LiquidCrystal-I2C",
        "${workspaceFolder}/ArduinoCore-avr/variants/mega",
        "${workspaceFolder}/ArduinoCore-avr/cores/arduino"
      ],
      "browse": {
        "path": [
          "${workspaceFolder}/tools/avr/include",
          "${workspaceFolder}/tools/avr/include/avr",
          "${workspaceFolder}/FreeRTOS/FreeRTOS-Kernel",
          "${workspaceFolder}/FreeRTOS/FreeRTOS-Kernel/include",
          "${workspaceFolder}/FreeRTOS/FreeRTOS-Kernel/portable/ThirdParty/GCC/ATmega",
          "${workspaceFolder}/FreeRTOS/FreeRTOS-Kernel/portable/MemMang",

          "${workspaceFolder}/ArduinoCore-avr/libraries/Wire/src",
          "${workspaceFolder}/ArduinoCore-avr/libraries/Wire/src/utility",
          "${workspaceFolder}/ArduinoCore-avr/libraries/Arduino-LiquidCrystal-I2C",
          "${workspaceFolder}/ArduinoCore-avr/variants/mega",
          "${workspaceFolder}/ArduinoCore-avr/cores/arduino"
        ],
        "limitSymbolsToIncludedHeaders": true
      }
    }
  ],
  "version": 4
}